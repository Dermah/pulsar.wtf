    <script>
      // 2. This code loads the IFrame Player API code asynchronously.
      var tag = document.createElement('script');
      tag.src = "http://www.youtube.com/player_api";
      var firstScriptTag = document.getElementsByTagName('script')[0];
      firstScriptTag.parentNode.insertBefore(tag, firstScriptTag);

      // 3. This function creates an <iframe> (and YouTube player)
      //    after the API code downloads.
      var player01;
      var player02;

      // 3. This function creates an <iframe> (and YouTube player)
      //    after the API code downloads.
      function onYouTubePlayerAPIReady() {
        player01 = new YT.Player('test-pattern-video', {
          playerVars: { 'autoplay': 1, 'controls': 1,'autohide':1,'wmode':'opaque', 'rel':0 },
          videoId: 'cXAevrYhicI',
          height: 720,
          width:  1280,
          events: {
            'onReady': onPlayerReady,
          }
        });

        player02 = new YT.Player('mpe-video', {
          playerVars: { 'loop': 1, 'autoplay': 1, 'controls': 1,'autohide':1,'wmode':'opaque', 'rel':0 },
          videoId: 'CeKHksYxmY8',
          height: 720,
          width:  1280,
          events: {
            'onReady': onPlayerReady,
            'onStateChange': loopPlayer
          }
        });
      }

      // 4. The API will call this function when the video player is ready.
      function onPlayerReady(event) {
        event.target.mute();
      }

      function loopPlayer(event) {
        if (event.data === YT.PlayerState.ENDED) {
          event.target.playVideo();
        }
      }
    </script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/reveal.js/3.8.0/js/reveal.js"></script>
    <script src="<%= Routes.static_path(@conn, "/js/slides.js") %>"></script>
  </body>
</html>
